<Users>
    name:"users"

    M_AKSilverAppbar:
        id: app_bar
        opacity:0
        max_height: dp(250)
        pin_top: True
        hide_toolbar: True
        radius: [dp(50),0,0,0]
        toolbar_bg: app.theme_cls.primary_color
        t_opacity:0

        M_AKSilverAppbarHeader:
            orientation: 'vertical'
            FitImage:
                id: product
                count: 0
                source: ''
                on_count:
                    root.change_product(self.count)

        M_AKSilverAppbarContent:
            padding: dp(10)
            id: content
            elevation: 8
            size_hint_y: None
            height: self.minimum_height
            orientation: 'vertical'
            md_bg_color: app.theme_cls.primary_color

    Label:
        id: icolabel
        markup: True
        text:f"{icon('icon-users', int(dp(128)), '45459a', 'fontello')}"